{
  "name": "umutcangungormus/laravel-named-route-binding",
  "description": "Bind route parameters to controller method arguments by name instead of order",
  "keywords": ["laravel", "route", "binding", "parameters", "named"],
  "license": "MIT",
  "type": "library",
  "authors": [
    {
      "name": "Umutcan Güngörmüş",
      "email": "umutcangungormus@example.com"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "laravel/framework": "^10.0|^11.0|^12.0",
    "orchestra/testbench": "^8.0|^9.0",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "UmutcanGungormus\\NamedRouteBinding\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "UmutcanGungormus\\NamedRouteBinding\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "UmutcanGungormus\\NamedRouteBinding\\NamedRouteBindingServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}